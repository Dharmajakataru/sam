<?xml version="1.0" encoding="utf-8"?>
<saw:report xmlns:saw="com.siebel.analytics.web/report/v1.1" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlVersion="201201160" cacheControl="bypassCache"><saw:criteria xsi:type="saw:physicalCriteria" connectionPool="dwh"><saw:columns><saw:column columnID="c0" xsi:type="saw:physicalColumnRef"><saw:columnHeading><saw:caption><saw:text>REASON_FOR_EXCLUSION</saw:text></saw:caption></saw:columnHeading></saw:column><saw:column columnID="c1" xsi:type="saw:physicalColumnRef"><saw:columnHeading><saw:caption><saw:text>DHF_AREA</saw:text></saw:caption></saw:columnHeading></saw:column><saw:column columnID="c2" xsi:type="saw:physicalColumnRef"><saw:columnHeading><saw:caption><saw:text>UPRN</saw:text></saw:caption></saw:columnHeading></saw:column><saw:column columnID="c3" xsi:type="saw:physicalColumnRef"><saw:columnHeading><saw:caption><saw:text>PROPERTY_STREET_ADDRESS</saw:text></saw:caption></saw:columnHeading></saw:column><saw:column columnID="c4" xsi:type="saw:physicalColumnRef"><saw:columnHeading><saw:caption><saw:text>SUBURB</saw:text></saw:caption></saw:columnHeading></saw:column><saw:column columnID="c5" xsi:type="saw:physicalColumnRef"><saw:columnHeading><saw:caption><saw:text>STATE</saw:text></saw:caption></saw:columnHeading></saw:column><saw:column columnID="c6" xsi:type="saw:physicalColumnRef"><saw:columnHeading><saw:caption><saw:text>POST_CODE</saw:text></saw:caption></saw:columnHeading></saw:column><saw:column columnID="c7" xsi:type="saw:physicalColumnRef" aggRule="sum"><saw:displayFormat><saw:formatSpec suppress="repeat" wrapText="true"><saw:dataFormat xsi:type="saw:number" commas="true" negativeType="minus" minDigits="0" maxDigits="0"/></saw:formatSpec></saw:displayFormat><saw:tableHeading/><saw:columnHeading><saw:caption fmt="html"><saw:text>CAPITAL_VALUE</saw:text></saw:caption><saw:displayFormat><saw:formatSpec/></saw:displayFormat></saw:columnHeading></saw:column><saw:column columnID="c8" xsi:type="saw:physicalColumnRef"><saw:columnHeading><saw:caption><saw:text>REP_HEADER</saw:text></saw:caption></saw:columnHeading></saw:column></saw:columns><saw:sql>select case when dp1.dwelling_type_code not in (&#39;APAR&#39;, &#39;UNIT&#39;, &#39;SRA&#39;, &#39;HOUS&#39;, &#39;TOWN&#39;) then
    &#39;Dwelling type code is &#39; || nvl(dwelling_type_code, &#39;NULL&#39;)
else
    decode(rd.suburb, null, &#39;Suburb/Post Code not covered in RISMARK data&#39;, &#39;Property did not meet criteria at time of data load&#39;) 
end as reason_for_exclusion,
 dp1.dhf_area, dp1.uprn, dp1.property_street_address, dp1.addr_line2 as suburb, dp1.addr_line3 as state, dp1.post_code, mv.capital_value, &#39;Total capital Value of excluded properties: &#39; as rep_header from 
    (
    select dp.* from dfl.dim_property dp 
    left outer join dfl.f_capital_value_projection fcvp
    on dp.uprn = fcvp.uprn and fcvp.forecast_date = &#39;31-Dec-2015&#39;
    where 
    dp.property_class = &#39;1&#39; 
    and dp.ownership in (&#39;DHA Investment&#39;, &#39;DHA Investment MCA &#39;, &#39;DHA Inventory&#39;, &#39;DHA Inventory MCA &#39;)
    and fcvp.uprn is null
    ) dp1
    --
    left outer join 
        (
        select distinct postcode, suburb, property_type from dfl.rismark_data where forecast_date = &#39;31-Dec-2015&#39;
        ) rd 
    on dp1.post_code = rd.postcode and dp1.addr_line2 = rd.suburb 
    and rd.property_type = decode(dp1.dwelling_type_code, &#39;APAR&#39;, &#39;U&#39;, &#39;UNIT&#39;, &#39;U&#39;, &#39;SRA&#39;, &#39;U&#39;, &#39;HOUS&#39;, &#39;H&#39;, &#39;TOWN&#39;, &#39;H&#39;, &#39;X&#39;)
    --
    left outer join 
        (
        select post_code, suburb from dfl.residex_data where forecast_date = (select max(forecast_date) 
        from dfl.residex_data where forecast_date &lt;= to_date(&#39;31/12/2015&#39;, &#39;dd/mm/yyyy&#39;))
        group by post_code, suburb
        ) rd2
    on rd2.post_code = dp1.post_code and rd2.suburb = dp1.addr_line2
    --
    left outer join dfl.mv_dim_valuation mv
    on dp1.uprn = mv.uprn and mv.latest_valuation = &#39;Y&#39;
order by dhf_area, suburb
</saw:sql></saw:criteria><saw:interactionOptions drill="true" movecolumns="true" sortcolumns="true" addremovevalues="true" groupoperations="true" calcitemoperations="true" showhidesubtotal="true" showhiderunningsum="true" inclexclcolumns="true" hidecolumns="true"/><saw:views currentView="0" textDelivery="compoundView!1" includeNewColumns="true"><saw:view xsi:type="saw:compoundView" name="compoundView!1"><saw:cvTable><saw:cvRow><saw:cvCell viewName="titleView!1"><saw:displayFormat><saw:formatSpec/></saw:displayFormat></saw:cvCell></saw:cvRow><saw:cvRow><saw:cvCell viewName="narrativeView!1"/></saw:cvRow><saw:cvRow><saw:cvCell viewName="tableView!2"><saw:displayFormat><saw:formatSpec borderPosition="none"/></saw:displayFormat></saw:cvCell></saw:cvRow><saw:cvRow><saw:cvCell viewName="tableView!1"><saw:displayFormat><saw:formatSpec/></saw:displayFormat></saw:cvCell></saw:cvRow></saw:cvTable></saw:view><saw:view xsi:type="saw:titleView" name="titleView!1" includeName="false" startedDisplay="none"><saw:title><saw:caption fmt="text"><saw:text>Valid Properties excluded from Capital Value Forecast</saw:text></saw:caption></saw:title></saw:view><saw:view xsi:type="saw:tableView" name="tableView!1" rowsPerPage="250"><saw:edges><saw:edge axis="page" showColumnHeader="true"/><saw:edge axis="section"/><saw:edge axis="row" showColumnHeader="true"><saw:displayGrandTotals><saw:displayGrandTotal id="t1" grandTotalPosition="after"/></saw:displayGrandTotals><saw:edgeLayers><saw:edgeLayer type="column" columnID="c1"><saw:levels><saw:level><saw:displaySubTotal id="t2" subTotalPosition="after"/></saw:level></saw:levels></saw:edgeLayer><saw:edgeLayer type="column" columnID="c0"/><saw:edgeLayer type="column" columnID="c2"/><saw:edgeLayer type="column" columnID="c3"/><saw:edgeLayer type="column" columnID="c4"/><saw:edgeLayer type="column" columnID="c5"/><saw:edgeLayer type="column" columnID="c6"/><saw:edgeLayer type="column" columnID="c7"/></saw:edgeLayers></saw:edge><saw:edge axis="column"/></saw:edges></saw:view><saw:view xsi:type="saw:mapview" name="mapview!1"><saw:mapLayout><saw:splitterLayout orientation="horizontal"/></saw:mapLayout><saw:mapWidgets><saw:mapToolBar><saw:panTools><saw:panHand display="true"/></saw:panTools><saw:zoomTools><saw:zoomIn display="true"/><saw:zoomOut display="true"/></saw:zoomTools><saw:selectionTools><saw:pointTool display="true"/></saw:selectionTools></saw:mapToolBar><saw:mapInformation><saw:scaleInfo display="true"/><saw:overview display="true" viewState="collapsed"/></saw:mapInformation><saw:mapOverlay><saw:panButtons display="true"/><saw:zoomSlider display="true"/></saw:mapOverlay></saw:mapWidgets><saw:infoLegend display="true" viewState="collapsed"/><saw:viewportInfo/><saw:spatialLayers/></saw:view><saw:view xsi:type="saw:tableView" name="tableView!2"><saw:edges><saw:edge axis="page" showColumnHeader="true"/><saw:edge axis="section"/><saw:edge axis="row" showColumnHeader="false"><saw:edgeLayers><saw:edgeLayer type="column" columnID="c8"><saw:memberFormat><saw:displayFormat><saw:formatSpec fontSize="14" wrapText="true" borderPosition="none"/></saw:displayFormat></saw:memberFormat></saw:edgeLayer><saw:edgeLayer type="column" columnID="c7" aggRule="sum"><saw:memberFormat><saw:displayFormat><saw:formatSpec fontStyle="bold" fontSize="14" wrapText="true" borderPosition="none" fontColor="#FF0000"/></saw:displayFormat></saw:memberFormat></saw:edgeLayer></saw:edgeLayers></saw:edge><saw:edge axis="column"/></saw:edges></saw:view><saw:view xsi:type="saw:narrativeView" name="narrativeView!1"/><saw:view xsi:type="saw:sqlview" name="sqlview!1"/><saw:view xsi:type="saw:noresultsview" name="noresultsview!1"><saw:headline><saw:caption><saw:text>Capital Value Exclusions</saw:text></saw:caption></saw:headline><saw:detail><saw:caption><saw:text>All eligible properties have been included in the forecast model.</saw:text></saw:caption></saw:detail></saw:view></saw:views></saw:report>