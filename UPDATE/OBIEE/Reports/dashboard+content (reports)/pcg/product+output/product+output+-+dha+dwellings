(function (d) {
	var pvt = d.getElementsByClassName(&quot;PTChildPivotTable&quot;);
	var fycs=null;
	
	for (var h=0;h&lt;pvt.length; h++) {

		var tbl = pvt[h].firstChild;
		var disp = (tbl.rows[8].cells[0].innerHTML===&#39;Total of All Lots&#39;);

		var k=0, k2=0, x=0;

		for (var i = 0, row; row = tbl.rows[i]; i++) {
		   //iterate through rows
		   for (var j = 0, col; col = row.cells[j]; j++) {
				args = col.innerHTML;
				if(args === (&quot; Prior&quot;)) {
					console.log(&#39;Prior found at row &#39; + i + &#39;, col &#39; + j);
					console.log(&#39;colspan = &#39; + row.cells[j+1].colSpan);
					fycs = row.cells[j+1].colSpan + 2;
				}

				//if(i==7 || i==12 || i==17) row.cells[j].style.borderBottom = &quot;thin solid #000000&quot;;
				if((disp &amp;&amp; i==5) || (!disp &amp;&amp; (i==7 || i==12 || i==17))) row.cells[j].style.borderBottom = &quot;thin solid #000000&quot;;

			}  
		
			
			k=((!disp &amp;&amp; (i==2 || i==3 || i==8 || i==13)) || (disp &amp;&amp; (i==2 || i==3 || i==6)))?fycs:fycs-1;

			if(i&gt;1) {x=4; k2=k-2} else {if(i==0){x=1} else {x=2; k2=k-1}} 
			
			console.log(&#39;k=&#39;+k);
			row.cells[k].style.borderRight = &quot;thin solid #FF0000&quot;;
			if(i&gt;0) row.cells[k2].style.borderRight = &quot;thin solid #000000&quot;;

			for(var m=1;m&lt;=5;m++) {
				if(i&gt;0) row.cells[k2+(x*m)].style.borderRight = &quot;thin solid #000000&quot;;
				if(m&lt;5) row.cells[k+(x*m)].style.borderRight = &quot;thin solid #FF0000&quot;;
			}

			if((!disp &amp;&amp; (i==3 || i==8 || i==13)) || (disp &amp;&amp; i==3)) row.cells[0].style.borderBottom = &quot;thin solid #000000&quot;;
				
		}
	}
})(document);
&lt;/script&gt;</saw:text></saw:caption></saw:staticText></saw:view></saw:views></saw:report>