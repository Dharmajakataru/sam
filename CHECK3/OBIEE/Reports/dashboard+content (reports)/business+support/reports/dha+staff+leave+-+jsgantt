&lt;script type=&quot;text/javascript&quot;&gt;
function getColour(a) {
	var cl=&quot;&quot;;
	switch(a) {
		case &#39;RECREATIONAL&#39;: 
			cl = &#39;gtaskblue&#39;;
			break;
		case &#39;MATERNITY&#39;: 
			cl = &#39;gtaskyellow&#39;;
			break;
		case &#39;SPECIAL&#39;: 
			cl = &#39;gtaskred&#39;;
			break;
		case &#39;SICK/CARERS&#39;: 
			cl = &#39;gtaskgreen&#39;;
			break;
		case &#39;PARENT&#39;: 
			cl = &#39;gtaskpink&#39;;
			break;
	}
	return cl;
};

var c=1;
var x=&quot;&quot;;
var g = new JSGantt.GanttChart(document.getElementById(&#39;GanttChartDIV&#39;), &#39;day&#39;);
g.setDayMajorDateDisplayFormat(&#39;dd/mm/yy&#39;);
g.setCaptionType(&#39;Complete&#39;);
g.setDateTaskDisplayFormat(&#39;day dd month yyyy&#39;);
g.setWeekMajorDateDisplayFormat(&#39;yyyy&#39;);
g.setWeekMinorDateDisplayFormat(&#39;dd mon&#39;);
g.setShowTaskInfoLink(1);
g.setShowEndWeekDate(0);
g.setUseSingleCell(10000);
g.setFormatArr(&#39;Day&#39;, &#39;Week&#39;);
g.setShowComp(0);
g.setShowTaskInfoNotes(0);
g.setShowTaskInfoComp(0);
g.setUseSort(0);
g.setWeekColWidth(75);
g.setShowRes(0);
g.setShowDur(0);
g.setShowComp(0);
g.setShowStartDate(0);
g.setShowEndDate(0);


var id=1;
var emp=&quot;&quot;, prevEmp=&quot;&quot;;
var parentId=0, empc=1;
var sdate, edate, ltype;
var complete=&#39;N&#39;;

function f_process_row(at1, at4, at5, at6) {
emp = at1.replace(/&amp;quot;/g, &#39;&quot;&#39;).replace(/&amp;amp;/g, &#39;&amp;&#39;).replace(/&amp;#39;/g, &quot;&#39;&quot;);

if(id&gt;1) {
	if(emp!== prevEmp) {
		if(empc==1) {
			//write line without parent
			g.AddTaskItem(new JSGantt.TaskItem(prevId, prevEmp, sdate,  edate, getColour(ltype),&#39;&#39;, 0, ltype, 0,  0,0,1,&#39;&#39;,&#39;&#39;,&#39;Notes&#39;,g));
		} else {
			//write line with parent
			g.AddTaskItem(new JSGantt.TaskItem(prevId, prevEmp, sdate,  edate, getColour(ltype),&#39;&#39;, 0, ltype, 0,  0, parentId,1,&#39;&#39;,&#39;&#39;,&#39;Notes&#39;,g));
		}

		parentId = id;
		empc = 1; //reset count of rows for employee
		prevEmp = emp;

		
	} else {
		//multiple rows for single employee
		//check if this is second row so first row needs to written as parent
		if(empc==1) {
			//write empty parent line
			g.AddTaskItem(new JSGantt.TaskItem(prevId,  prevEmp,      sdate,           edate,          getColour(ltype),  &#39;&#39;,       0, ltype,  0,  2, 0,  1, &#39;&#39;,      &#39;&#39;,      &#39;&#39;,      g));
			parentId = prevId;
			g.AddTaskItem(new JSGantt.TaskItem(id, prevEmp, sdate,  edate, getColour(ltype),&#39;&#39;, 0, ltype, 0,  0,parentId,1,&#39;&#39;,&#39;&#39;,&#39;Notes&#39;,g));
			id++;
		} else {
			//write line with parent
			g.AddTaskItem(new JSGantt.TaskItem(prevId, prevEmp, sdate,  edate, getColour(ltype),&#39;&#39;, 0, ltype, 0,  0,parentId,1,&#39;&#39;,&#39;&#39;,&#39;Notes&#39;,g));
		}
		empc++;
	}
} else {
	parentId = id;
}

ltype = at4;
sdate = at5;
edate = at6;
prevEmp = emp;
prevId =id;
id++;

}</saw:text></saw:caption></saw:prefix><saw:narrative rowCount="1000"><saw:caption fmt="html"><saw:text>f_process_row(&#39;@{c7ee029d56354593f}&#39;, &#39;@{c78616adcfbec87fb}&#39;, &#39;@{c09b5a618238907c7}&#39;, &#39;@{c90ee4a8fd862f055}&#39;);</saw:text></saw:caption></saw:narrative><saw:postfix><saw:caption fmt="html"><saw:text>complete=&#39;Y&#39;;
f_process_row(emp, ltype, sdate, edate);
g.Draw();

&lt;/script&gt;
