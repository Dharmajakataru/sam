saw:tableHeading/><saw:columnHeading><saw:caption fmt="html"><saw:text>Previous House Value</saw:text></saw:caption><saw:displayFormat><saw:formatSpec/></saw:displayFormat></saw:columnHeading></saw:column><saw:column columnID="c27" xsi:type="saw:physicalColumnRef"><saw:displayFormat><saw:formatSpec suppress="repeat" wrapText="true"><saw:dataFormat xsi:type="saw:number" commas="false" negativeType="minus" minDigits="0" maxDigits="0"/></saw:formatSpec></saw:displayFormat><saw:tableHeading/><saw:columnHeading><saw:caption fmt="html"><saw:text>Previous Capital Value</saw:text></saw:caption><saw:displayFormat><saw:formatSpec/></saw:displayFormat></saw:columnHeading></saw:column><saw:column columnID="c28" xsi:type="saw:physicalColumnRef"><saw:displayFormat><saw:formatSpec suppress="repeat" wrapText="true"><saw:dataFormat xsi:type="saw:number" commas="false" negativeType="minus" minDigits="0" maxDigits="0"/></saw:formatSpec></saw:displayFormat><saw:tableHeading/><saw:columnHeading><saw:caption fmt="html"><saw:text>Previous Rent Value</saw:text></saw:caption><saw:displayFormat><saw:formatSpec/></saw:displayFormat></saw:columnHeading></saw:column><saw:column columnID="c29" xsi:type="saw:physicalColumnRef"><saw:displayFormat><saw:formatSpec suppress="repeat" wrapText="true"/></saw:displayFormat><saw:tableHeading/><saw:columnHeading><saw:caption fmt="html"><saw:text>Previous Yield %</saw:text></saw:caption><saw:displayFormat><saw:formatSpec/></saw:displayFormat></saw:columnHeading></saw:column><saw:column columnID="c30" xsi:type="saw:physicalColumnRef"><saw:displayFormat><saw:formatSpec suppress="suppress" wrapText="true"/></saw:displayFormat><saw:tableHeading/><saw:columnHeading><saw:caption fmt="html"><saw:text>New Valuation ID</saw:text></saw:caption><saw:displayFormat><saw:formatSpec/></saw:displayFormat></saw:columnHeading></saw:column><saw:column columnID="c31" xsi:type="saw:physicalColumnRef"><saw:displayFormat><saw:formatSpec suppress="suppress" wrapText="true"/></saw:displayFormat><saw:tableHeading/><saw:columnHeading><saw:caption fmt="html"><saw:text>New Valuation Date</saw:text></saw:caption><saw:displayFormat><saw:formatSpec/></saw:displayFormat></saw:columnHeading></saw:column><saw:column columnID="c32" xsi:type="saw:physicalColumnRef"><saw:displayFormat><saw:formatSpec suppress="suppress" wrapText="true"/></saw:displayFormat><saw:tableHeading/><saw:columnHeading><saw:caption fmt="html"><saw:text>New Valuation Reason</saw:text></saw:caption><saw:displayFormat><saw:formatSpec/></saw:displayFormat></saw:columnHeading></saw:column><saw:column columnID="c33" xsi:type="saw:physicalColumnRef"><saw:displayFormat><saw:formatSpec suppress="suppress" wrapText="true"/></saw:displayFormat><saw:tableHeading/><saw:columnHeading><saw:caption fmt="html"><saw:text>New Valuation Type</saw:text></saw:caption><saw:displayFormat><saw:formatSpec/></saw:displayFormat></saw:columnHeading></saw:column><saw:column columnID="c34" xsi:type="saw:physicalColumnRef"><saw:displayFormat><saw:formatSpec suppress="suppress" wrapText="true"/></saw:displayFormat><saw:tableHeading/><saw:columnHeading><saw:caption fmt="html"><saw:text>New Valuer</saw:text></saw:caption><saw:displayFormat><saw:formatSpec/></saw:displayFormat></saw:columnHeading></saw:column><saw:column columnID="c35" xsi:type="saw:physicalColumnRef"><saw:displayFormat><saw:formatSpec suppress="suppress" wrapText="true"/></saw:displayFormat><saw:tableHeading/><saw:columnHeading><saw:caption fmt="html"><saw:text>New Inspected</saw:text></saw:caption><saw:displayFormat><saw:formatSpec/></saw:displayFormat></saw:columnHeading></saw:column><saw:column columnID="c36" xsi:type="saw:physicalColumnRef"><saw:displayFormat><saw:formatSpec suppress="suppress" wrapText="true"/></saw:displayFormat><saw:tableHeading/><saw:columnHeading><saw:caption fmt="html"><saw:text>New Valuation Status</saw:text></saw:caption><saw:displayFormat><saw:formatSpec/></saw:displayFormat></saw:columnHeading></saw:column><saw:column columnID="c37" xsi:type="saw:physicalColumnRef"><saw:displayFormat><saw:formatSpec suppress="repeat" wrapText="true"><saw:dataFormat xsi:type="saw:number" commas="false" negativeType="minus" minDigits="0" maxDigits="0"/></saw:formatSpec></saw:displayFormat><saw:tableHeading/><saw:columnHeading><saw:caption fmt="html"><saw:text>New Land Value</saw:text></saw:caption><saw:displayFormat><saw:formatSpec/></saw:displayFormat></saw:columnHeading></saw:column><saw:column columnID="c38" xsi:type="saw:physicalColumnRef"><saw:displayFormat><saw:formatSpec suppress="repeat" wrapText="true"><saw:dataFormat xsi:type="saw:number" commas="false" negativeType="minus" minDigits="0" maxDigits="0"/></saw:formatSpec></saw:displayFormat><saw:tableHeading/><saw:columnHeading><saw:caption fmt="html"><saw:text>New House Value</saw:text></saw:caption><saw:displayFormat><saw:formatSpec/></saw:displayFormat></saw:columnHeading></saw:column><saw:column columnID="c39" xsi:type="saw:physicalColumnRef"><saw:displayFormat><saw:formatSpec suppress="repeat" wrapText="true"><saw:dataFormat xsi:type="saw:number" commas="false" negativeType="minus" minDigits="0" maxDigits="0"/></saw:formatSpec></saw:displayFormat><saw:tableHeading/><saw:columnHeading><saw:caption fmt="html"><saw:text>New Capital Value</saw:text></saw:caption><saw:displayFormat><saw:formatSpec/></saw:displayFormat></saw:columnHeading></saw:column><saw:column columnID="c40" xsi:type="saw:physicalColumnRef"><saw:displayFormat><saw:formatSpec suppress="repeat" wrapText="true"><saw:dataFormat xsi:type="saw:number" commas="false" negativeType="minus" minDigits="0" maxDigits="0"/></saw:formatSpec></saw:displayFormat><saw:tableHeading/><saw:columnHeading><saw:caption fmt="html"><saw:text>New Rent Value</saw:text></saw:caption><saw:displayFormat><saw:formatSpec/></saw:displayFormat></saw:columnHeading></saw:column><saw:column columnID="c41" xsi:type="saw:physicalColumnRef"><saw:displayFormat><saw:formatSpec suppress="repeat" wrapText="true"/></saw:displayFormat><saw:tableHeading/><saw:columnHeading><saw:caption fmt="html"><saw:text>New Yield %</saw:text></saw:caption><saw:displayFormat><saw:formatSpec/></saw:displayFormat></saw:columnHeading></saw:column><saw:column columnID="c42" xsi:type="saw:physicalColumnRef"><saw:displayFormat><saw:formatSpec suppress="repeat" wrapText="true"/></saw:displayFormat><saw:tableHeading/><saw:columnHeading><saw:caption fmt="html"><saw:text>Capital Variation %</saw:text></saw:caption><saw:displayFormat><saw:formatSpec/></saw:displayFormat></saw:columnHeading></saw:column><saw:column columnID="c43" xsi:type="saw:physicalColumnRef"><saw:displayFormat><saw:formatSpec suppress="repeat" wrapText="true"><saw:dataFormat xsi:type="saw:number" commas="false" negativeType="minus" minDigits="0" maxDigits="0"/></saw:formatSpec></saw:displayFormat><saw:tableHeading/><saw:columnHeading><saw:caption fmt="html"><saw:text>$ Rent Variation</saw:text></saw:caption><saw:displayFormat><saw:formatSpec/></saw:displayFormat></saw:columnHeading></saw:column><saw:column columnID="c44" xsi:type="saw:physicalColumnRef"><saw:displayFormat><saw:formatSpec suppress="repeat" wrapText="true"/></saw:displayFormat><saw:tableHeading/><saw:columnHeading><saw:caption fmt="html"><saw:text>Rent Variation %</saw:text></saw:caption><saw:displayFormat><saw:formatSpec/></saw:displayFormat></saw:columnHeading></saw:column></saw:columns><saw:sql>select p.uprn , 
p.property_street_address,  
p.addr_line2 as suburb,
p.addr_line3 as state,
p.post_code,
dma.management_area_code as management_area,
dra.rent_code as rent_area,
dpa.area as dhf_area,
dp1.descriptor_value as ownership,
dp2.descriptor_value as asset_status,
chk.c20_check_code as sr_classification,
dp3.descriptor_value as beds,
dp4.descriptor_value as rent_bill,
dpd.rcl as client_rent,
p.current_agreement_type,
dl.lease_edition,
lm.first_lease_rent as commencement_rent,
fl.rent as current_lease_rent,
to_char(fl.lease_start_date, &#39;dd/mm/yyyy&#39;) as lease_start_date,
to_char(fl.current_lease_end_date, &#39;dd/mm/yyyy&#39;) as current_lease_end_date,
-- 
to_char(prev.valuation_date, &#39;dd/mm/yyyy&#39;) as prev_val_date, prev.reason as prev_val_reason, prev.type as prev_val_type, 
prev.valuer as prev_valuer, prev.physical_inspection as prev_inspected, prev.land_value as prev_land_value, 
prev.house_value as prev_house_value, prev.capital_value as prev_capital_value, prev.market_rent as prev_rent_value, 
decode(prev.capital_value, 0, 0, round((((prev.market_rent/7) * 365.25) / prev.capital_value) * 100, 3)) as prev_yield_perc,
--
to_char(v.valuation_id) as new_val_id, 
to_char(v.valuation_date, &#39;dd/mm/yyyy&#39;) as new_val_date, v.reason as new_val_reason, v.type as new_val_type, v.valuer as new_valuer, 
v.physical_inspection as new_inspected, v.authorised_by as new_val_status, v.land_value as new_land_value, v.house_value as new_house_value, v.capital_value as new_capital_value,
v.market_rent as new_rent_value, 
decode(v.capital_value, 0, 0, round((((v.market_rent/7) * 365.25) / v.capital_value) * 100, 3)) as new_yield_perc,
decode(prev.capital_value, 0, 0, round((((v.capital_value - prev.capital_value) / prev.capital_value) * 100), 3)) as perc_capital_variation,
v.market_rent - prev.market_rent as doll_rent_variation,
decode(prev.market_rent, 0, 0, round((((v.market_rent - prev.market_rent) / prev.market_rent) * 100), 3)) as perc_rent_variation
--
from
dfl.mv_dim_valuation v
--
left outer join (select property_id, valuation_date, max(latest_cr_start_date) as latest_cr_start_date from dfl.mv_dim_valuation group by property_id, valuation_date) prev1
on v.property_id = prev1.property_id and prev1.valuation_date = TO_DATE(&#39;@{PV_PREV_VAL_DATE}{31/12/2012}&#39;, &#39;DD/MM/YYYY&#39;)
--
left outer join dfl.mv_dim_valuation prev 
on prev1.property_id = prev.property_id and prev1.valuation_date = prev.valuation_date and nvl(prev1.latest_cr_start_date, &#39;01-Jan-1901&#39;) = nvl(prev.latest_cr_start_date, &#39;01-Jan-1901&#39;)
--
left outer join dfl.dim_property_desc dpd on v.property_id = dpd.uprn
--
left outer join dfl.dim_property p on p.uprn = v.property_id
--
left outer join dfl.fact_property fp on fp.uprn = p.uprn
--
left outer join dfl.dim_management_area dma on dma.management_area_id = fp.management_area_id
--
left outer join dfl.dim_rent_area dra on dra.rent_area_id = fp.rental_area_id
--
left outer join dfl.dim_property_areas dpa on p.uprn = dpa.uprn
--
left outer join dfl.dim_prop1_chklist chk on p.uprn = chk.uprn
--
left outer join dfl.dim_prop_descriptor_hist dp1 
on dp1.uprn = p.uprn and dp1.descriptor = &#39;POWN&#39; and dp1.end_effect_date is null and dp1.current_flag = &#39;Y&#39;
--
left outer join dfl.dim_prop_descriptor_hist dp2 
on dp2.uprn = p.uprn and dp2.descriptor = &#39;ASST&#39; and dp2.end_effect_date is null and dp2.current_flag = &#39;Y&#39;
--
left outer join dfl.dim_prop_descriptor_hist dp3 
on dp3.uprn = p.uprn and dp3.descriptor = &#39;BEDS&#39; and dp3.end_effect_date is null and dp3.current_flag = &#39;Y&#39;
--
left outer join dfl.dim_prop_descriptor_hist dp4 
on dp4.uprn = p.uprn and dp4.descriptor = &#39;RNTB&#39; and dp4.end_effect_date is null and dp4.current_flag = &#39;Y&#39;
--
left outer join dfl.dim_lease dl on dl.lease_id = fp.lease_id
--
left outer join dfl.fact_lease fl on fl.lease_ref = dl.lease_ref
--
left outer join (select lm1.lease_ref, lm1.amount as first_lease_rent from dfl.DIM_LEASE_CHARGES_HIST lm1
      where lm1.charge_item_code = &#39;RENT&#39;
      and lm1.effect_date = 
        (select min(lm2.effect_date) from dfl.DIM_LEASE_CHARGES_HIST lm2
        where lm2.lease_ref = lm1.lease_ref and lm2.charge_item_code = &#39;RENT&#39; and (lm2.end_effect_date is null
        or end_effect_date &gt; effect_date))
        and (lm1.end_effect_date is null or lm1.end_effect_date &gt; lm1.effect_date)) lm
on dl.lease_ref = lm.lease_ref
--
where 
v.valuation_date = TO_DATE(&#39;@{PV_CURR_VAL_DATE}{31/12/2013}&#39;, &#39;DD/MM/YYYY&#39;)
</saw:sql></saw:criteria><saw:views currentView="0"><saw:view xsi:type="saw:compoundView" name="compoundView!1"><saw:cvTable><saw:cvRow><saw:cvCell viewName="titleView!1"><saw:displayFormat><saw:formatSpec/></saw:displayFormat></saw:cvCell></saw:cvRow><saw:cvRow><saw:cvCell viewName="htmlview!1"/></saw:cvRow><saw:cvRow><saw:cvCell viewName="tableView!1"><saw:displayFormat><saw:formatSpec/></saw:displayFormat></saw:cvCell></saw:cvRow><saw:cvRow><saw:cvCell viewName="htmlview!2"/></saw:cvRow></saw:cvTable></saw:view><saw:view xsi:type="saw:titleView" name="titleView!1"/><saw:view xsi:type="saw:tableView" name="tableView!1" scrollingEnabled="false"><saw:edges><saw:edge axis="page" showColumnHeader="true"/><saw:edge axis="section"/><saw:edge axis="row" showColumnHeader="true"><saw:edgeLayers><saw:edgeLayer type="column" columnID="c0"/><saw:edgeLayer type="column" columnID="c1"/><saw:edgeLayer type="column" columnID="c2"/><saw:edgeLayer type="column" columnID="c3"/><saw:edgeLayer type="column" columnID="c4"/><saw:edgeLayer type="column" columnID="c5"/><saw:edgeLayer type="column" columnID="c6"/><saw:edgeLayer type="column" columnID="c7"/><saw:edgeLayer type="column" columnID="c8"/><saw:edgeLayer type="column" columnID="c9"/><saw:edgeLayer type="column" columnID="c10"/><saw:edgeLayer type="column" columnID="c11"/><saw:edgeLayer type="column" columnID="c12"/><saw:edgeLayer type="column" columnID="c13"/><saw:edgeLayer type="column" columnID="c14"/><saw:edgeLayer type="column" columnID="c15"/><saw:edgeLayer type="column" columnID="c16"/><saw:edgeLayer type="column" columnID="c17"/><saw:edgeLayer type="column" columnID="c18"/><saw:edgeLayer type="column" columnID="c19"/><saw:edgeLayer type="column" columnID="c20"/><saw:edgeLayer type="column" columnID="c21"/><saw:edgeLayer type="column" columnID="c22"/><saw:edgeLayer type="column" columnID="c23"/><saw:edgeLayer type="column" columnID="c24"/><saw:edgeLayer type="column" columnID="c25"/><saw:edgeLayer type="column" columnID="c2