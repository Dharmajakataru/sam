&lt;script type=&quot;text/javascript&quot;&gt;
function dha_apply() {
    var applyButton;
	
	var els = document.getElementsByTagName(&quot;A&quot;);
	for (i = 0; i &lt; els.length; i++) {
		if(els[i].innerHTML == &quot;Apply&quot;) {
			applyButton = els[i];
		}
	}

	if(applyButton) {
		applyButton.click();
	} else {
		alert(&quot;No Apply button found&quot;);
	}
}

&lt;/script&gt; 
&lt;input value=&quot;Apply Comments&quot; type=&quot;button&quot; onclick=&quot;dha_apply()&quot;&gt;
