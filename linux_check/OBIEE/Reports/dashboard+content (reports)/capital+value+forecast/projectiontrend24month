function fmtCells() {
	var hdrid = new Array();
	var tds = document.getElementsByClassName(&quot;wibble&quot;);
	var start = tds[0].parentNode.id;
	hdrid = start.split(&quot;_&quot;);
	var rowid = hdrid[4];
	var colid = hdrid[5];

	var cell, pcell, val, pval;
	
	for(var r=0;r&lt;1000;r++) {
		
		for(var c=0;c&lt;1000;c++) {
			cell = document.getElementById(&quot;db_saw_&quot; + hdrid[2] + &#39;_&#39; + hdrid[3] + &#39;_&#39; + r + &#39;_&#39; + c);
			if(cell) {
				val = parseInt(cell.childNodes[0].innerHTML);
//				if(c&gt;0 &amp;&amp; val &lt; pval) cell.style.backgroundColor=&#39;#FF704D&#39;;
//				if(c&gt;0 &amp;&amp; val &gt; pval) cell.style.backgroundColor=&#39;#D9FF66&#39;;
				if(c&gt;0 &amp;&amp; val &lt; pval) cell.style.color=&#39;#FF0000&#39;;
				if(c&gt;0 &amp;&amp; val &gt; pval) cell.style.color=&#39;#008000&#39;;
			} else { 
				if(c==0) r=1000; 
                                break;
			}
			pcell = cell;
			pval = val;
		}
	}
}
fmtCells();
