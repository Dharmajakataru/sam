<saw:view xmlns:saw="com.siebel.analytics.web/report/v1.1" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="saw:globalFilterPrompt" name=""><saw:prompts scope="dashboard" subjectArea="&quot;PCG&quot;"><saw:promptStep setWidthToAllPrompts="true" autoApplyPrompt="true" showResetButton="false"><saw:individualPrompts><saw:prompt xsi:type="saw:columnFilterPrompt" columnID="c1" subjectArea="&quot;PCG&quot;" required="false"><saw:formula><sawx:expr xmlns:sawx="com.siebel.analytics.web/expression/v1.1" xsi:type="sawx:sqlExpression">&quot;Bank Guarantee Facts&quot;.&quot;Value&quot;</sawx:expr></saw:formula><saw:promptOperator op="in"/><saw:promptUIControl xsi:type="saw:dropDown" maxChoices="1" includeAllChoices="false"><saw:customWidth width="120" using="custompixels"/></saw:promptUIControl><saw:label><saw:caption><saw:text>PV_FIRST_DEL_YEAR</saw:text></saw:caption></saw:label><saw:promptDefaultValues type="sql" usingCodeValue="false"><saw:promptDefaultValue>SELECT
&quot;Estate Master&quot;.&quot;D Period - Cashflow&quot;.&quot;Fin Year&quot;  s_1,
 &quot;F DM CashFlow Unit Monthly&quot;.&quot;DM Unit&quot; s_2,
   1  as s_3
FROM &quot;Estate Master&quot;
WHERE
(&quot;D DM Code&quot;.&quot;Category Description&quot; = &#39;Sales and Income Revenue&#39;) AND 
(&quot;D DM Cash Flow&quot;.&quot;Portfolio Name&quot; in (&#39;DHA Full Finance Portfolio&#39;, &#39;Project Rebaselines&#39;)) AND 
(&quot;D DM Code&quot;.&quot;PCG FIN Report Section Name&quot; = &#39;Revenue&#39;) AND 
(&quot;D Development Project&quot;.&quot;Summary&quot; = &#39;@{PV_DEV_PROJECT}&#39;) AND 
(&quot;D Period - Cashflow&quot;.&quot;Fin Year&quot; &gt;=  VALUEOF(&quot;RV_CURRENT_FIN_YEAR&quot;)) AND 
(&quot;F DM CashFlow Unit Monthly&quot;.&quot;DM Unit&quot; &lt;&gt; 0)
UNION select VALUEOF(&quot;RV_CURRENT_FIN_YEAR&quot;), 0,2 FROM &quot;Estate Master&quot;
ORDER BY s_3, s_1 asc, s_2 desc</saw:promptDefaultValue></saw:promptDefaultValues><saw:constrainPrompt type="none" autoSelectValue="default"/><saw:setPromptVariables><saw:setPromptVariable location="value" type="presentationVariable" variableFormula="PV_FIRST_DEL_YEAR"/></saw:setPromptVariables><saw:promptSource xsi:type="saw:allChoices"/></saw:prompt><saw:prompt xsi:type="saw:columnFilterPrompt" columnID="c2" subjectArea="&quot;PCG&quot;" required="false"><saw:formula><sawx:expr xmlns:sawx="com.siebel.analytics.web/expression/v1.1" xsi:type="sawx:sqlExpression">&quot;D Action Item&quot;.&quot;Id&quot;</sawx:expr></saw:formula><saw:promptOperator op="in"/><saw:promptUIControl xsi:type="saw:dropDown" maxChoices="1" includeAllChoices="false"><saw:customWidth width="120" using="custompixels"/></saw:promptUIControl><saw:label><saw:caption><saw:text>PV_FIRST_DEL_YEAR_MINUS_1</saw:text></saw:caption></saw:label><saw:promptDefaultValues type="sql" usingCodeValue="false"><saw:promptDefaultValue>SELECT @{PV_FIRST_DEL_YEAR} -1 
FROM &quot;Estate Master&quot;
</saw:promptDefaultValue></saw:promptDefaultValues><saw:constrainPrompt type="none"/><saw:setPromptVariables><saw:setPromptVariable location="value" type="presentationVariable" variableFormula="PV_FIRST_DEL_YEAR_MINUS_1"/></saw:setPromptVariables><saw:promptSource xsi:type="saw:allChoices"/></saw:prompt><saw:prompt xsi:type="saw:columnFilterPrompt" columnID="c4" subjectArea="&quot;PCG&quot;" required="false"><saw:formula><sawx:expr xmlns:sawx="com.siebel.analytics.web/expression/v1.1" xsi:type="sawx:sqlExpression">&quot;D Action Item&quot;.&quot;Meeting Id&quot;</sawx:expr></saw:formula><saw:promptOperator op="in"/><saw:promptUIControl xsi:type="saw:dropDown" maxChoices="1" includeAllChoices="false"><saw:customWidth width="120" using="custompixels"/></saw:promptUIControl><saw:label><saw:caption><saw:text>PV_FIRST_DEL_YEAR_PLUS_1</saw:text></saw:caption></saw:label><saw:promptDefaultValues type="sql" usingCodeValue="false"><saw:promptDefaultValue>SELECT @{PV_FIRST_DEL_YEAR} +1 
