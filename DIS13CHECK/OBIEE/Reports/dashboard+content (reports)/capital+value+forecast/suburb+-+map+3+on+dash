function onFeatureSelect(event) {
	var feature = event.feature;
	var content = &#39;&lt;br/&gt;&#39;+&#39;&lt;br/&gt;&#39;
	content+= &#39;&lt;table class=&quot;bordered&quot;&gt;&lt;tr&gt;&lt;th&gt;Suburb:&lt;/th&gt;&lt;td&gt;&#39;+feature.attributes.suburb+&#39;&lt;/td&gt;&lt;/tr&gt;&#39;;
	content+= &#39;&lt;th&gt;Postcode:&lt;/th&gt;&lt;td&gt;&#39;+feature.attributes.postcode+&#39;&lt;/td&gt;&lt;/tr&gt;&#39;;
	content+= &#39;&lt;th&gt;State:&lt;/th&gt;&lt;td&gt;&#39;+feature.attributes.state+&#39;&lt;/td&gt;&lt;/tr&gt;&#39;;
	content+= &#39;&lt;th&gt;Projected growth:&lt;/th&gt;&lt;td&gt;&#39;+feature.attributes.projection+&#39;%&lt;/td&gt;&lt;/tr&gt;&#39;;
	content+= &#39;&lt;/table&gt;&#39;;
	//content +=feature.attributes.suburb + &#39;&lt;br/&gt;&#39;+feature.attributes.postcode+ &#39;&lt;br/&gt;&#39;+feature.attributes.state+&#39;&lt;br/&gt;&#39;+feature.attributes.projection;

	popup = new EMS.Popup(&quot;chicken&quot;, 
							 feature.geometry.getBounds().getCenterLonLat(),
							 new OpenLayers.Size(200,200),
							 content,
							 null, true, onPopupClose);
	//popup.setOpacity(0.8);
	feature.popup = popup;
	map.addPopup(popup);

}
function onFeatureUnselect(event) {
	var feature = event.feature;
	if(feature.popup) {
		map.removePopup(feature.popup);
		feature.popup.destroy();
		delete feature.popup;
	}
}	


