      <saw:staticText>
        <saw:caption fmt="html" captionID="kcap1564723711063_42225" oldID="kcap1564723511853_11503">
          <saw:text>&lt;script type=&quot;text/javascript&quot; src=&quot;../custom_styles/js/dha/whereis-maps/jquery.min.js&quot;&gt;&lt;/script&gt;
&lt;script type=&quot;text/javascript&quot;&gt;
$(document).ready((function (d) {
	var pvt = d.getElementsByClassName(&quot;PTChildPivotTable&quot;);
	var fycs=null;
	var border1 = &quot;thin solid #000000&quot;;	//start fiscal year default
	var border2 = &quot;thin solid #FF0000&quot;; //start fiscal year default
	
	for (var h=0;h&lt;pvt.length; h++) {

		var tbl = pvt[h].firstChild;

		if (tbl.rows.length ==20) {
			var prov11g = (tbl.rows[19].cells[0].innerHTML===&#39;Grand Total&#39;);
			var prov12c = (tbl.rows[3].cells[0].firstChild.innerHTML===&#39;Defence MCA&#39;);
			var prov = (prov11g || prov12c);
			
			if(start_cal_year ==&#39;Y&#39;) {
				border1 = &quot;thin solid #FF0000&quot;;
				border2 = &quot;thin solid #000000&quot;;
			}

			if(prov) {
				//loop for vertical red lines around calendar years
				for (var i = 0, row; row = tbl.rows[i]; i++) {
					for (var j = 0, col; col = row.cells[j]; j++) {
						if(i==0 &amp;&amp; start_cal_year ==&#39;Y&#39;) {if(j&gt;1 &amp;&amp; j&lt;8) {row.cells[j].style.borderRight = border1} }
						if(i==1) {if(j&gt;1 &amp;&amp; j&lt;13 &amp;&amp; (j%2==0)) {row.cells[j].style.borderRight = border1} }
						if(i==2 || i==3 || i==8 || i==13 || i==18) {if ((j==2) || (j&gt;1 &amp;&amp; j&lt;23 &amp;&amp; j%4==2)) {row.cells[j].style.borderRight = border1}}
						if((i&gt;3 &amp;&amp; i&lt;8) || (i&gt;8 &amp;&amp; i&lt;13) || (i&gt;13 &amp;&amp; i&lt;18) || i==19) {if ((j==1) || (j&gt;1 &amp;&amp; j&lt;22 &amp;&amp; j%4==1)) {row.cells[j].style.borderRight = border1}}
					}
				}


				//loop for vertical black lines around fiscal years
				for (var i = 0, row; row = tbl.rows[i]; i++) {
					for (var j = 0, col; col = row.cells[j]; j++) {
						if(i==0 &amp;&amp; start_cal_year ==&#39;Y&#39;) {if(j&gt;2 &amp;&amp; j&lt;8) {row.cells[j].style.borderRight = border1} }
						if(i==1) {if(j&gt;2 &amp;&amp; j&lt;12 &amp;&amp; (j%2==1)) {row.cells[j].style.borderRight = border2} }
						if(i==2 || i==3 || i==8 || i==13 || i==18) {if ((j&gt;3 &amp;&amp; j&lt;21 &amp;&amp; j%4==0)) {row.cells[j].style.borderRight = border2}}
						if((i&gt;3 &amp;&amp; i&lt;8) || (i&gt;8 &amp;&amp; i&lt;13) || (i&gt;13 &amp;&amp; i&lt;18) || i==19) {if ((j==3) || (j&gt;6 &amp;&amp; j&lt;20 &amp;&amp; j%4==3)) {row.cells[j].style.borderRight = border2}}

						//horizontal separating lines
						if((i==3 || i==8 || i==13) &amp;&amp; j==0) row.cells[j].style.borderBottom = &quot;thin solid #000000&quot;;
						if(i==7 || i==12 || i==17 || i==18) row.cells[j].style.borderBottom = &quot;thin solid #000000&quot;;
					}
				}

			}
		}
	}
})(document));
